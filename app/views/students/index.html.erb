<h1>Student Names</h1>
<ul>
  <% @students.each do |student| %>
  <li><%= link_to student.name, student_path(student) %>
    <%=  link_to 'Edit', edit_student_path(student)%> 
    <%= link_to 'Delete', student_path(student), method: 'delete' %> 
  </li>
  <%end %>

  <%= link_to "Create Student", new_student_path %>
</ul>
